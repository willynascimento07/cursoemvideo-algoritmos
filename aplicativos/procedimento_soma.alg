algoritmo "semnome"
// Função :
// Autor :
// Data : 22/04/2020
// Seção de Declarações 
var
    x, y: inteiro
procedimento soma(a, b: inteiro)
inicio
      escreval("Recbi o valor ", a)
      escreval("Recbi o valor ", b)
      escreval("A soma entre os dois é: ", a + b)
fimprocedimento
inicio
// Seção de Comandos 
       x <- 5
       y <- 3
       soma(x, y)
fimalgoritmo
